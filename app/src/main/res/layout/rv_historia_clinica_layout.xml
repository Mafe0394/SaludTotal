<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>
        <variable
            name="citaMedicaModelo"
            type="com.wigilabs.saludtotal.entities.CitaMedicaModel" />
    </data>

    <LinearLayout
        android:id="@+id/ly_historia_clinica_select"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/lb_rv_medical_Especiality"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@{citaMedicaModelo.especialidad}"/>
        <TextView
            android:id="@+id/lb_rv_patient"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@{citaMedicaModelo.especialidad}"/>
        <TextView
            android:id="@+id/lb_rv_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@{citaMedicaModelo.especialidad}"/>

    </LinearLayout>
</layout>